{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": "off",
        "linebreak-style": "off",
        "no-console": "off",
        "eol-last": "off",
        "space-before-function-paren": "off"
    }
}
